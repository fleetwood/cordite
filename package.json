    "@tailwindcss/forms": "^0.5.6",
    "axios": "^1.5.1",
    "dayjs": "^1.11.10",
    "next": "12.2.5",
    "next-auth": "^4.23.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-json-pretty": "^2.2.0",
    "react-markdown": "9.0.0",
