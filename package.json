    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@prisma/client": "5.4.1",
    "@types/node": "20.8.0",
    "@types/react": "18.2.23",
    "autoprefixer": "^10.4.16",
