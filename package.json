{
  "name": "hello-next",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "pnpm pr-gen && next build",
    "check-test": "npx checkly test --record",
    "check-deploy": "npx checkly deploy",
    "clean": "rm -rf .next",
    "dev": "clear && next dev",
    "dev:debug": "next dev --inspect-brk=5858",
    "pr-studio": "npx prisma studio",
    "pr-migrate": "npx prisma migrate dev",
    "pr-format": "npx prisma format",
    "pr-pull": "npx prisma db pull",
    "pr-gen-edge": "npx prisma generate --data-proxy",
    "pr-gen": "npx prisma generate",
    "pr-reset": "pnpm pr-migrate && pnpm pr-pull && pnpm pr-format && pnpm pr-gen",
    "start": "next start",
    "tw": "npx tailwindcss -i ./components/styles/cordite.css -o ./dist/cordite.css --watch",
    "watch": "tsc -w",
    "watcher": "pnpm clean && pnpm watch"
  },
  "keywords": [],
